                <Label Content="Время начала:" HorizontalAlignment="Left" Margin="10,98,0,0" VerticalAlignment="Top" FontSize="14"/>
                <Label Content="Время конца:" HorizontalAlignment="Left" Margin="10,132,0,0" VerticalAlignment="Top" FontSize="14"/>
                <xctk:TimePicker x:Name="TimeStart" Margin="137,92,10,183" AllowTextInput="False" EndTime="20:0:0" Maximum="12/31/9999 20:00:00" StartTime="8:0:0" AllowSpin="False" Format="LongTime"/>
                <Button x:Name="AdderWorkerGraphic" Content="Записать" Margin="108,215,102,0" VerticalAlignment="Top" Height="37" Click="AdderWorkerGraphic_Click"/>
                <Button x:Name="ClosePanel_Worker_Craphic" Content="Закрыть" HorizontalAlignment="Left" Margin="108,257,0,0" VerticalAlignment="Top" Width="197" Height="37" Click="ClosePanel_Worker_Craphic_Click"/>
                <TextBox x:Name="FieldNameWorkerGraphic" HorizontalAlignment="Left" Height="23" Margin="137,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="260" FontSize="14" PreviewTextInput="FieldNameRole_PreviewTextInput" PreviewKeyDown="Login_PreviewKeyDown"/>
