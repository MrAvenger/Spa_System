            <CheckBox x:Name="CheckYval_Users" Content="Отображать деактивированных пользователей" HorizontalAlignment="Left" Margin="660,4,0,0" VerticalAlignment="Top" Height="19" Width="286" Unchecked="CheckYval_Users_Unchecked" Checked="CheckYval_Users_Unchecked" Visibility="Hidden"/>
            <Grid x:Name="Panel_Add_WorkerFunction_Graphic" HorizontalAlignment="Left" Height="304" Margin="1068,66,0,0" VerticalAlignment="Top" Width="407" Background="#FFDDF4F4" Visibility="Hidden">
