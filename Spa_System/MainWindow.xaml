                <Button x:Name="AdderClient" Content="Записать" Margin="72,297,64,0" VerticalAlignment="Top" Height="37" Click="AdderClient_Click"/>
                <Label Content="Имя" HorizontalAlignment="Left" Margin="1,67,0,0" VerticalAlignment="Top" FontSize="14"/>
                <TextBox x:Name="FnameClient" HorizontalAlignment="Left" Height="23" Margin="90,73,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="243" FontSize="14" PreviewTextInput="FieldNameRole_PreviewTextInput" PreviewKeyDown="Login_PreviewKeyDown"/>
